
<div class="card">
    <img src = {{this.image_url}} class="card-img-top" alt=`Picture of {{this.name}}` />
    <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <p class="card-text tagline">{{this.tagline}}</p>
    <p class="card-text">{{this.description}}</p>
    </div>
</div>
 